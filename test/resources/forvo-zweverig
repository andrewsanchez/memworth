<h3>Accents &amp; languages on <span>maps</span></h3>
<div class="mapsbord">
	<div id="map" style="height: 225px;"></div>
</div>

<script type="text/javascript">
var pronunciations = [
	['<strong style="font-size:120%; margin-bottom:1px; display:block; max-width:200px">  zweverig in <span style="color:#04A300">Dutch<\/span><\/strong>  <span style="font-size:100%"> Pronunciation by <a href="https://forvo.com/user/n52e3"/ >n52e3<\/a> <br/> <span class="lang_xx">(Male from Netherlands)<\/span><\/span>',52.152737,4.504150,'2139424','OTE1ODc1NC8xMTgvOTE1ODc1NF8xMThfMjM1ODEzNy5tcDM=','OTE1ODc1NC8xMTgvOTE1ODc1NF8xMThfMjM1ODEzNy5vZ2c=','good','dC9oL3RoXzkxNTg3NTRfMTE4XzIzNTgxMzcubXAz','dC9oL3RoXzkxNTg3NTRfMTE4XzIzNTgxMzcub2dn','h']
];
var markers = [];

var map = L.map('map').setView([0, 0], 8);

var LeafIcon = L.Icon.extend({
	options: {
		iconSize: [32, 42],
		iconAnchor: [10, 35],
		popupAnchor: [-9, -42]
	}
});

var url = 'https://maps.forvo.com/osm_tiles/{z}/{x}/{y}.png';
L.tileLayer(url, {
	attribution: 'Map data &copy; <a href="http://openstreetmap.org">OpenStreetMap</a> contributors, <a href="http://creativecommons.org/licenses/by-sa/2.0/">CC-BY-SA</a>',
	maxZoom: 4
}).addTo(map);

var getMinOrMax = function (markersObj, minOrMax, latOrLng) {
	return _[minOrMax](markersObj, function (value) {
		if (value.marker) {
			var latLng = value.marker.getLatLng();
			return latLng[latOrLng];
		}
	})[latOrLng];
};

var getBounds = function (markersObj) {
	var maxLat = getMinOrMax(markersObj, 'max', 'lat');
	var minLat = getMinOrMax(markersObj, 'min', 'lat');
	var maxLng = getMinOrMax(markersObj, 'max', 'lng');
	var minLng = getMinOrMax(markersObj, 'min', 'lng');

	var southWest = new L.LatLng(minLat, minLng);
	var northEast = new L.LatLng(maxLat, maxLng);
	return new L.LatLngBounds(southWest, northEast);
}

var fitToBounds = function (markersObj) {
	var bounds = getBounds(markersObj);
	if (markersObj.length > 1) {
		map.fitBounds(bounds);
	} else {
		map.fitBounds(bounds);
		map.setZoom(8);
	}
}

var createMarker = function(latitude, longitude, type, content, id, path_mp3, path_ogg, path_audio_mp3, path_audio_ogg, quality) {
	var icon;
	if ('low' == type) {
		icon = new LeafIcon({ iconUrl: '/_presentation/img/ico_tip_low.png' });
	} else {
		icon = new LeafIcon({ iconUrl: '/_presentation/assets/img/shapes/mapplay.png' });
	}

	var marker = new L.marker([latitude, longitude], { icon: icon });
	marker.bindPopup(content);
	marker.on('click', function () {
		Play(id, path_mp3, path_ogg, true, path_audio_mp3, path_audio_ogg, quality);
	});
	marker.addTo(map);

	marker.category = type;

	return marker;
}

var loadMarkers = function () {
	var marker = [];
	for (var i = 0; i < pronunciations.length; i++) {
		var pronunciation = pronunciations[i];

		marker = createMarker(pronunciation[1], pronunciation[2], pronunciation[6], pronunciation[0], pronunciation[3], pronunciation[4], pronunciation[5],pronunciation[7],pronunciation[8],pronunciation[9]);
		markers.push({ 'lat': pronunciation[1], 'lng': pronunciation[2], 'id': pronunciation[3], 'marker': marker });
	}

	fitToBounds(markers);
}

loadMarkers();
</script>
